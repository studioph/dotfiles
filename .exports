export LC_ALL="en_US.UTF-8";

export DOTNET_CLI_TELEMETRY_OPTOUT=true
export FF_PROFILE="$HOME/.var/app/org.mozilla.firefox/.mozilla/firefox/studiop"

STARSHIP_PATH="$HOME/.local/bin/starship"

eval "$($STARSHIP_PATH init bash)"
