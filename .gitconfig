[user]
    name = Paul Hutchings
[filter "lfs"]
    clean = git-lfs clean -- %f
    smudge = git-lfs smudge -- %f
    process = git-lfs filter-process
    required = true
[credential]
    helper = /usr/local/share/gcm-core/git-credential-manager-core
    credentialStore = secretservice
[core]
    editor = codium
[init]
    defaultBranch = main
